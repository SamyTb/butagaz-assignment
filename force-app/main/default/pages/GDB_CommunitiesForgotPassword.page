        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        <script>
            $(
                function(){
                    var items = $('.password-validation ul li');
                    var passwordFields = $('input[type=password]')
                    var passwordField = passwordFields.first();
                    var confirmationPasswordField = passwordFields.last();
                    
                    passwordFields.keyup(
                        function(){
                            var lastItem = items.last().removeClass().addClass('form__error-messages');
                            var confirmation = confirmationPasswordField.val();
                            if(confirmation == passwordField.val() && confirmation){
                                lastItem.addClass('form__success-messages');
                            }
                        }
                    );
                            
                    passwordField.keyup(
                        function(){
                            var field = $(this);
                            var value = field.val();
                            var indexesToHighlight = [];
                            
                            items.removeClass().addClass('form__error-messages');
                            
                            if(value){
                                if(value.length >= 8){
                                    indexesToHighlight.push(0);
                                }
                                
                                if(/[a-z]+/.test(value)){
                                    indexesToHighlight.push(1);
                                }
                                
                                if(/[A-Z]+/.test(value)){
                                    indexesToHighlight.push(2);
                                }
                                
                                if(/\d+/.test(value)){
                                    indexesToHighlight.push(3);
                                }
                                
                                if(/[ `!@#$%^&*()_+\-=\[\]{};':"\\|,.<>\/?~]/.test(value)){
                                    indexesToHighlight.push(4);
                                }
                                
                                if(value == confirmationPasswordField.val() && value){
                                    indexesToHighlight.push(5);
                                }
                            }
                            
                            for(var i = 0; i < indexesToHighlight.length ; i++)
                            {
                                var indexToHighlight = indexesToHighlight[i];
                                items.eq(indexToHighlight).addClass('form__success-messages');
                            }
                        }
                    );
                    
                    passwordFields.keyup();
                }
            );
        </script>
    </body>
